{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules":{
    "one-variable-per-declaration": false, 
    "no-console": false
  },
  "jsRules": {
    "no-console": false 
  }
}
